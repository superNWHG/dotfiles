<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE RSyntaxTheme SYSTEM "theme.dtd">

<!--
	RSyntaxTextArea's default theme.
	See theme.dtd and org.fife.ui.rsyntaxtextarea.Theme for more information.
-->
<RSyntaxTheme version="1.0">

   <!-- Omitting baseFont will use a system-appropriate monospaced. -->
   <!--<baseFont family="..." size="13"/>-->

   <!--  General editor colors. -->
   <background color="282A36"/>
   <caret color="F8F8F0"/>
   <selection fg="F8F8F2" bg="424450" />
   <currentLineHighlight color="44475A" fade="false"/>
   <marginLine fg="b0b4b9"/>
   <markAllHighlight color="ffc800"/>
   <markOccurrencesHighlight color="d4d4d4" border="false"/>
   <matchedBracket fg="F8F8F0" bg="282A36" highlightBoth="false" animate="false"/>
   <hyperlinks fg="2C7BB6"/>
   <secondaryLanguages>
      <language index="1" bg="fff0cc"/>
      <language index="2" bg="dafeda"/>
      <language index="3" bg="ffe0f0"/>
   </secondaryLanguages>

   <!-- Gutter styling. -->
   <gutterBorder color="282A36"/>
   <lineNumbers fg="6272A4"/>
   <foldIndicator fg="6272A4" iconBg="282A36"/>
   <iconRowHeader activeLineRange="3399ff"/>

   <!-- Syntax tokens. -->
   <tokenStyles>
      <style token="IDENTIFIER" fg="F8F8F2"/>
      <style token="DATA_TYPE" fg="8BE9FD" bold="true"/>
      <style token="FUNCTION" fg="50FA7B"/>
      <style token="VARIABLE" fg="8BE9FD" bold="true"/>
      <style token="RESERVED_WORD" fg="8BE9FD" bold="true"/>
      <style token="RESERVED_WORD_2" fg="8BE9FD" bold="false"/>
      <style token="PREPROCESSOR" fg="FF79C6" bold="false"/>

      <style token="ANNOTATION" fg="6272A4"/>
      <style token="COMMENT_DOCUMENTATION" fg="6272A4"/>
      <style token="COMMENT_EOL" fg="6272A4"/>
      <style token="COMMENT_MULTILINE" fg="6272A4"/>
      <style token="COMMENT_KEYWORD" fg="6272A4" bold="true"/>
      <style token="COMMENT_MARKUP" fg="6272A4"/>
      <style token="LITERAL_BOOLEAN" fg="BD93F9" bold="false"/>
      <style token="LITERAL_NUMBER_DECIMAL_INT" fg="BD93F9"/>
      <style token="LITERAL_NUMBER_FLOAT" fg="BD93F9"/>
      <style token="LITERAL_NUMBER_HEXADECIMAL" fg="BD93F9"/>
      <style token="LITERAL_STRING_DOUBLE_QUOTE" fg="F1FA8C"/>
      <style token="LITERAL_CHAR" fg="FFB86C"/>
      <style token="LITERAL_BACKQUOTE" fg="F1FA8C"/>

      <style token="OPERATOR" fg="FF79C6"/>
      <style token="REGEX" fg="8BE9FD"/>
      <style token="SEPARATOR" fg="50FA7B"/>
      <style token="WHITESPACE" fg="BD93F9"/>
      <style token="ERROR_IDENTIFIER" fg="FF5555" />
      <style token="ERROR_NUMBER_FORMAT" fg="FF5555" />
      <style token="ERROR_STRING_DOUBLE" fg="FF5555" />
      <style token="ERROR_CHAR" fg="FF5555" />
   </tokenStyles>

</RSyntaxTheme>
